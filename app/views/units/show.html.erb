  <div id="meat">
    </br>
    <div class="row hide-for-small">
      <div class="Large-12 columns">
        <ul class="button-group even-6">
          <li><%= link_to("Main Page", root_path, class:"medium button")%></li>
          <li><%= link_to("Home E", unit_path(5), class:"medium button")%></li>
          <li><%= link_to("Home D", unit_path(4), class:"medium button")%></li>
          <li><%= link_to("Home C", unit_path(3), class:"medium button")%></li>
          <li><%= link_to("Home A", unit_path(1), class:"medium button")%></li>
          <li><%= link_to("Home B", unit_path(2), class:"medium button")%></li>
        </ul>
      </div>
    </div>  
    <div class="row show-for-small">
      <div class="small-12 columns" >
        <ul id="small_align" class="button-group even-6">
          <li><%= link_to("\<", root_path, class:"medium button")%></li>
          <li><%= link_to("E", unit_path(5), class:"medium button")%></li>
          <li><%= link_to("D", unit_path(4), class:"medium button")%></li>
          <li><%= link_to("C", unit_path(3), class:"medium button")%></li>
          <li><%= link_to("A", unit_path(1), class:"medium button")%></li>
          <li><%= link_to("B", unit_path(2), class:"medium button")%></li>          
        </ul>
      </div>
    </div>
    <div class="row">
      <div class="large-3 large-offset-1 columns">
        <h1><strong>Home <%= @unit.letter %> </strong></h1>
        <ul style="list-style: none">
          <li><h4><%= @unit.size %>SF</h4></li>
          <li><h4><%= @unit.rooms %> Bedrooms</h4></li>
          <li><h4><%= @unit.bathrooms %> Bathrooms</h4></li>
          <li><h4><%= p "Attached Garage" if @unit.garage %></h4></li>
        </ul>
      </div>
      <div class="large-6 large-offset-1 end columns">
        <%= image_tag(@unit.overview) %>
      </div>
    </div>
    </br>
    </br>
    <div class="row">
      <div class="large-6 large-offset-1 columns">
        <%= image_tag(@unit.front) %>
        <h5>Homes feature unique modern interior designs with beautiful open floorplans.</h5>
        </br>
        </br>
        </br>
      <p>All information provided should be independently verified by buyer.</p>
      </div>
      <div class="large-4 columns end">
        <div class="orbit-container">
          <ul data-orbit data-options="pause_on_hover: false; animation: slide; animation_speed: 1000; timer_speed: 5000; slide_number: false; navigation_arrows: true">
            <li><%= link_to(image_tag(@unit.floor1), '') %></li>
            <li><%= link_to(image_tag(@unit.floor2), '') %></li>
            <li><%= link_to(image_tag(@unit.floor3), '') %></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
